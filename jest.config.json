{
  "verbose": true,
  "clearMocks": true,
  "restoreMocks": true,
  "collectCoverage": true,
  "setupFilesAfterEnv": ["./config/enzyme.js"],
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}",
    "!**/node_modules/**",
    "!src/index.tsx",
    "!src/store/**",
    "!src/reducers/**",
    "!src/routes/**",
    "!src/sagas/**",
    "!src/config/**"
  ]
}
